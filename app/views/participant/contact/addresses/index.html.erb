<h3><%="#{I18n.t(:listing)} #{I18n.t(:participant_contact_address)}"%></h3>

<table>
  <tr>
    <th><%=I18n.t(:participant_person)%></th>
    <th><%=I18n.t(:place)%></th>
    <th><%=I18n.t(:type)%></th>
    <th><%=I18n.t(:remark)%></th>
    <th><%=I18n.t(:active)%></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @participant_contact_addresses.each do |participant_contact_address| %>
  <tr>
    <td><%= participant_contact_address.participant_person_id %></td>
    <td><%= participant_contact_address.address_place_id %></td>
    <td><%= participant_contact_address.participant_contact_type_id %></td>
    <td><%= participant_contact_address.remark %></td>
    <td><%= participant_contact_address.active %></td>
    <td><%= link_to I18n.t(:show), participant_contact_address %></td>
    <td><%= link_to I18n.t(:edit), edit_participant_contact_address_path(participant_contact_address) %></td>
    <td><%= link_to I18n.t(:destroy), participant_contact_address, :confirm => I18n.t(:are_you_sure?), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

