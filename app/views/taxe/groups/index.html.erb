<h1>Listing taxe_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Remark</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @taxe_groups.each do |taxe_group| %>
  <tr>
    <td><%= taxe_group.name %></td>
    <td><%= taxe_group.remark %></td>
    <td><%= taxe_group.active %></td>
    <td><%= link_to I18n.t(:Show), taxe_group %></td>
    <td><%= link_to I18n.t(:Edit), edit_taxe_group_path(taxe_group) %></td>
    <td><%= link_to 'Destroy', taxe_group, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Group', new_taxe_group_path %>
