<h3><%="#{I18n.t(:listing)} #{I18n.t(:taxe_value)}"%></h3>
<table>
  <tr>
    <th><%= I18n.t(:taxe_type)%></th>
    <th><%= I18n.t(:percentage)%></th>
    <th><%= I18n.t(:basis)%></th>
    <th><%= I18n.t(:value)%></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @taxe_values.each do |taxe_value| %>
  <tr>
    <td><%= taxe_value.taxe_type_id %></td>
    <td><%= taxe_value.percentage %></td>
    <td><%= taxe_value.basis %></td>
    <td><%= taxe_value.value %></td>
    <td><%= link_to I18n.t(:show), taxe_value %></td>
    <td><%= link_to I18n.t(:edit), edit_taxe_value_path(taxe_value) %></td>
    <td><%= link_to I18n.t(:destroy), taxe_value, :confirm => I18n.t(:are_you_sure?), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br/>

<%= link_to I18n.t(:new), new_taxe_value_path %>
