<h1>Listing taxe_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Remark</th>
    <th>Active</th>
    <th>Taxe group</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @taxe_types.each do |taxe_type| %>
  <tr>
    <td><%= taxe_type.name %></td>
    <td><%= taxe_type.remark %></td>
    <td><%= taxe_type.active %></td>
    <td><%= taxe_type.taxe_group_id %></td>
    <td><%= link_to 'Show', taxe_type %></td>
    <td><%= link_to 'Edit', edit_taxe_type_path(taxe_type) %></td>
    <td><%= link_to 'Destroy', taxe_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

