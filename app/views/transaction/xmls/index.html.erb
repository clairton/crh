<h1><%= I18n.t(:Listing, :transaction_xmls)%></h1>
<p id="notice"><%= notice %></p>
<table>
  <tr>
    <th>Id</th>
    <th>Transaction record</th>
    <th>Name</th>
    <th>Active</th>
    <th>Remark</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @transaction_xmls.each do |transaction_xml| %>
  <tr>
    <td><%= transaction_xml.id %></td>
    <td><%= transaction_xml.transaction_record_id %></td>
    <td><%= transaction_xml.name %></td>
    <td><%= transaction_xml.active %></td>
    <td><%= transaction_xml.remark %></td>
    <td><%= link_to I18n.t :Show, transaction_xml %></td>
    <td><%= link_to I18n.t :Edit, edit_transaction_xml_path(transaction_xml) %></td>
    <td><%= link_to I18n.t :Destroy, transaction_xml, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to "#{I18n.t :New} #{I18n.t :Xml}", new_transaction_xml_path %>

